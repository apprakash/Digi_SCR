    "import json\n",
    "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
    "from tqdm import tqdm\n",
    "\n",
    "def process_chunk(item, chunker: SemanticChunker):\n",
    "    if item['HeadNote']:\n",
    "        headnote_length = len(item['HeadNote'])\n",
    "\n",
    "        new_items = []\n",
    "        if headnote_length > 1000:\n",
    "            chunks = chunker.split_content(item['HeadNote'])\n",
    "            for chunk in chunks:\n",
    "                new_item = item.copy()\n",
    "                new_item['content'] = chunk\n",
    "                new_items.append(new_item)\n",
    "        else:\n",
    "            new_item = item.copy()\n",
    "            new_item['content'] = item['HeadNote']\n",
    "            new_items.append(new_item)\n",
    "\n",
    "        return new_items\n",
    "    else:\n",
    "        return None\n",
    "\n",
    "def process_case_referred(item):\n",
    "    combined_info_list = []\n",
    "    if 'Case referred' in item and item['Case referred']:\n",
    "        for case in item['Case referred']:\n",
    "            combined_info = f\"{case['Judgment Name']} ({case['Judicial Consideration']})\"\n",
    "            combined_info_list.append(combined_info)\n",
    "        all_combined_info = '; '.join(combined_info_list)\n",
    "        item['Combined Info'] = all_combined_info\n",
    "    return item\n",
    "\n",
    "\n",
    "def process_file_parallel(input_file_path, output_file_path, chunker, max_workers=20):\n",
    "    with open(input_file_path, 'r', encoding='utf-8') as file:\n",
    "        data = json.load(file)\n",
    "\n",
    "    new_data = []\n",
    "    with ThreadPoolExecutor(max_workers=max_workers) as executor:\n",
    "        preprocessed_data = [process_case_referred(item) for item in data]\n",
    "        futures = [executor.submit(process_chunk, item, chunker) for item in preprocessed_data]\n",
    "        for future in tqdm(as_completed(futures), total=len(futures), desc=\"Processing\"):\n",
    "            processed_chunks = future.result()\n",
    "            if processed_chunks:\n",
    "                new_data.extend(processed_chunks)\n",
    "\n",
    "    with open(output_file_path, 'w', encoding='utf-8') as file:\n",
    "        json.dump(new_data, file, indent=4, ensure_ascii=False)"
   ]
  },
